
public class VarEx2 {

	public static void main(String[] args) {
		final int score; // = 100;
		score = 200;		
		boolean power = true;
		
		byte b = 127;	// -128 ~ 127
		
		int oct = 010;	// 8진수, 10진수로는 8
		int hex = 0x10;	// 16진수, 10진수로는 16
		
		long l = 1_000_000_000;
		
		System.out.println(oct);
		System.out.println(hex);


	}

}
